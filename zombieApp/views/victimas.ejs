<% include header %>
<div id="cartasVict">
<% if(entries.length) { %>
    <% entries.forEach((entry)=> { %>
        <div class="jumbotron">
                <h1 class="display-8"><%= entry.name %></h1>
            <p class="lead">
                <%= entry.address %>
                <br>
                <%= entry.phone %>
                <br>
                <%= entry.instagram %>
            </p>
        </div>
    <% }) %>
    </div>
<% } else { %>
    <p>No hay visitas todavía! <a href="/new-entry">Agregar una!</a></p>
<% } %>
<div id="btncentrar">
<a href="/new-entry" class="btn btn-primary pull-right">
Agrega una nueva víctima</a>
</div>
<% include footer %>